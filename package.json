{
  "name": "toy-robot",
  "version": "1.0.0",
  "description": " simulation of a toy robot moving on a square tabletop",
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r src/web/. dist/web/. && rm dist/web/index.ts",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "cli-dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest"
  },
  "type": "module",
  "author": "Nycolas Messias",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/parser": "^7.17.8",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
